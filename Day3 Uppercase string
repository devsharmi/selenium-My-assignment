package week1.day3;

public class PracticeString {

	public static void main(String[] args) {
 String Test ="Changeme";
char[] value = Test.toCharArray();
for (int i = 0; i < value.length; i++) {
	if (i%2==0) {
		System.out.print(value[i]);
	} else {
char a = Character.toUpperCase(value[i]);
System.out.print(a);
	}
}

	}

}
